<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.jfw.qms.entity.DiseaseClassification" table="disease_classification" schema="qms">
        <id name="diseaseId">
            <column name="disease_id" sql-type="int(11)"/>
        </id>
        <property name="diseaseName">
            <column name="disease_name" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="diseaseDescribe">
            <column name="disease_describe" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>